#include "ref.jazz"

export
fn keccakP1600(reg u64 ptr) {
  inline int x, round;
  reg u64 t;
  stack u64[25] state;
  stack u64[24] constants;
  for x = 0 to 25 {
    t = [ptr + 8 * x];
    state[x] = t;
  }
  constants = keccakRoundConstants(constants);
  state = permute(state, constants);
  for x = 0 to 25 {
    t = state[x];
    [ptr + 8 * x] = t;
  }
}
