
fn add (reg u64 x) -> reg u64 {
   reg u64 z;
   z = x;
   z += 1;
   return z;
}

fn add1 (reg u64 x) -> reg u64 {
   reg u64 z;
   z = x + 1;
   z += x;
   return z;
}

export fn main() -> reg u64 {
   reg u64 z1;
   reg u64 z2;
   reg u64[1] w;
   z1 = 10;

   z2 = add(z1);
   w[0] = add(z1);
   z1 = add(z1);

   z2 = add1(z1);
   /*w[0] = add1(z1);
   z1 = add1(z1); */

   z1 += w[0];
   z1 += z2;
   return z1; 
}

