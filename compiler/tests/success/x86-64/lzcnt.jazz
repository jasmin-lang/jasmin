fn lzcnt (reg u64 x) -> reg u64 {
   reg u64 r; 
   r = x;
   ?{}, r = #DEC(r);

   ?{}, r = #LZCNT(x);
   return r;
}

export fn foo () -> reg u64 {
  reg u64 r a;
  a = 1 << 62;
  r = lzcnt(a);
  return r;
}

export fn tzcnt(reg u64 x p) -> reg u16 {
  reg u32 y;
  reg u16 z;
  ?{}, x = #TZCNT([p]);
  ?{}, x = #TZCNT(x);
  ?{}, y = #TZCNT_32((u32)[p + 4 * x]);
  ?{}, y = #TZCNT_32(y);
  x = (64s)y;
  ?{}, z = #TZCNT_16((u16)[p + 2 * x]);
  ?{}, z = #TZCNT_16(z);
  return z;
}
