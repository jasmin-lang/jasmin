  b = #MOVZX_u32u8(b);
  c = #MOVZX_u64u16(b);
  c = #MOVZX_u64u8(c);

  [y] = c;

  a = #MOVZX_u16u8((u8)[y]);
  (u16)[y] = a;
  b = #MOVZX_u32u16((u16)[y]);
  (u32)[y] = b;
  b = #MOVZX_u32u8((u8)[y]);
  (u32)[y] = b;
  c = #MOVZX_u64u16((u16)[y]);
  [y] = c;
  c = #MOVZX_u64u8((u8)[y]);
