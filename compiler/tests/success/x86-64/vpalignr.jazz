export fn test_vpalignr_128(reg u64 cp ap bp)
{
  reg u128 c a b;
  a = (u128)[ap + 0];
  b = (u128)[bp + 0];
  c = #VPALIGNR_128(a,b,8);
  (u128)[cp + 0] = c;
}

