export 
fn fill(reg u64 io, reg u64 len) -> reg u64 {
   stack u64[10] t; 
   reg u64 i, max, aux;

   max = 10;
   max = len if len < max;

   i = 0;
   #[bounded]
   while (i < max) {
     // t[i] = [io + 8 * i];  // works but not in the spirit
     aux = [io + 8 * i];
     t[i] = aux;
     i += 1;
   }

   i = 0;
   while (i < max) {
     // [io + 8 * i] = t[i];
     aux = t[i];
     [io + 8 * i] = aux;
     i += 1; 
   }  

   return i;
}
