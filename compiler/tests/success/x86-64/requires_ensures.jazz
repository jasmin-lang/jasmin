#[safety =
   { requires = is_arr_init(t, 0, 10) && 0 <= i && i < 10
   , ensures = true}
 ]
export fn read_arr (reg ptr u32[10] t, reg u64 i) -> reg u32 {
  reg u32 r = t[i];
  return r;
}

#[safety =
   { requires = is_arr_init(t, 0, 10)
   , requires  = 0 <= i && i < 10
   , ensures = true}
 ]
export fn read_arr_ (reg ptr u32[10] t, reg u64 i) -> reg u32 {
  reg u32 r = t[i];
  return r;
}