#[ct = "public × public → secret × secret"]
export
fn foo (reg u64 x y) -> reg u64, reg u64 {
   reg u64 r1, r2;
   r1 = x;
   r2 = y;
   return r1, r2;
}

#[ct = "l1 × l2 → l1 × l2"]
export
fn foo2 (reg u64 x y) -> reg u64, reg u64 {
   reg u64 r1, r2;
   r1 = x;
   r2 = y;
   return r1, r2;
}

#[ct = "public × public → secret"]
export fn foo1 (reg u64 x, reg u64 y) -> reg u64 {
   reg u64 r1;
   r1 = x;
   r1 += y;
   return r1;
}
