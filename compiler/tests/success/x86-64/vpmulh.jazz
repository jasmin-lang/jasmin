export fn test (reg u64 in) { 
  reg u128 x y z;
  reg u256 xx yy zz;

  x = (u128)[in];
  y = x;
  z = #VPMULH_128(x,y);
  (u128)[in] = z;
  z = #VPMULHU_128(x,y);
  (u128)[in] = z;
  z = #VPMULHRS_128(x,y);
  (u128)[in] = z;

  xx = (u256)[in];
  yy = xx;
  zz = #VPMULH_256(xx,yy);
  (u256)[in] = zz;
  zz = #VPMULHU_256(xx,yy);
  (u256)[in] = zz;
  zz = #VPMULHRS_256(xx,yy);
  (u256)[in] = zz;
}
