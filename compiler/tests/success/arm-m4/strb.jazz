export
fn strb(reg u32 arg) {
    reg u32 x;
    x = arg;

    (u8)[arg] = x;

    (u8)[x + 3] = x;

    // Shifts.
    (u8)[x + (arg * 1)] = x;
    (u8)[x + (arg * 2)] = x;
    (u8)[x + (arg * 4)] = x;
    (u8)[x + (arg * 8)] = x;

    // Set flags.
    reg bool n, z, v, c;
    n, z, c, v = #CMP(x, 0);

    // Conditions.
    (u8)[x] = x if z;            // EQ
    (u8)[x] = x if !z;           // NE
    (u8)[x] = x if c;            // CS
    (u8)[x] = x if !c;           // CC
    (u8)[x] = x if n;            // MI
    (u8)[x] = x if !n;           // PL
    (u8)[x] = x if v;            // VS
    (u8)[x] = x if !v;           // VC
    (u8)[x] = x if c && !z;      // HI
    (u8)[x] = x if !c || z;      // LS
    (u8)[x] = x if n == v;       // GE
    (u8)[x] = x if n != v;       // LT
    (u8)[x] = x if !z && n == v; // GT
    (u8)[x] = x if z || n != v;  // LE

    (u8)[x + arg] = x if !z;
    (u8)[x + 3] = x if !z;
    (u8)[x - 3] = x if !z;
    (u8)[x + arg * 4] = x if !z;
}
