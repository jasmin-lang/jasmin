u64[4] glob_t = { 0, 1, 2, 3 };

export fn sum (reg u64 x) -> reg u64 {
  reg u64 r;
  reg u64 i;
  reg ptr u64[4] gt1 gt2;
  gt1 = glob_t;
  gt2 = gt1;
  r = 0; 
  i = 0;
  while (i < 4) {
    r += gt1[(int)i];
    i += 1;
  }
  i = 0;
  while (i < 4) {
    r += gt2[(int)i];
    i += 1;
  }   
  return r;
}

