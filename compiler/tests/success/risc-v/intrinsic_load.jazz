export
fn load(reg u32 arg) -> reg u32 {
    reg u32 a = #LOAD([:u8 arg]);

    reg u32 b = #LOAD_u8([:u8 arg]);
    reg u32 c = #LOAD_s8([:u8 arg]);

    reg u32 d = #LOAD_u16([:u16 arg]);
    reg u32 e = #LOAD_s16([:u16 arg]);

    reg u32 f = #LOAD_s32([:u32 arg]);

    reg u32 g = #LOAD_s32([:u32 arg + 2]);

    f ^= g;
    d ^= e;
    b ^= c;
    d ^= f;
    a ^= b;
    a ^= d;

    return a;
}
