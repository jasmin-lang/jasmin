fn f (reg ptr u64[2] r) -> reg ptr u64[2] {
  r[0] = 1;
  return r;
}

export fn test (reg ptr u64[4] r) -> reg ptr u64[4], reg u64 {
  reg ptr u64[2] r2;

  r2 = r[1:2];
  () = #spill(r);
  r2 = f(r2);
  () = #unspill(r); // we unspill r, while it is not fully valid
  r[1:2] = r2;

  reg u64 res;
  res = r[1];

  return r, res;
}
