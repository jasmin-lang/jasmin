/*
Test for intialising variable during declaration
The used semantic is the following :
reg u32 x=3;

is traduced to
reg u32 x;
x=3;
*/

fn test_basic() -> reg u32 {
    reg u32 x=3;
    reg u32 y=x;
    return y;
}

fn test_multiples () -> reg u32,reg u32 {
    reg u32 x=1,y=2;
    return x,y;
}

fn test_primitive(){
    reg u32[1] a;
    a[0] = 1;
    stack u32[1] b = #copy_32(a[0:1]);
}

fn test_calls () {
    reg u32  x= test_basic();
}

export fn sequence() -> reg u32 {
  reg u32 x = 1, y = x + 1;
  return y;
}
