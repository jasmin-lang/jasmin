export fn main(reg u64 x) -> reg u8 {
  inline int i;
  stack u8[65] s;
  for i = 0 to 65 { s[i] = i; }
  reg ptr u8[33] p = s[0:33];
  reg ptr u8[17] q = p[0:17];

  reg u64 a;
  ?{}, a = #POPCNT(x);
  reg u32 b;
  ?{}, b = #POPCNT_32(x);
  reg u16 c;
  ?{}, c = #POPCNT_16(x);

  reg u8 r;
  r = s[a];
  reg u64 tmp;
  tmp = (64u) b;
  r += p[tmp];
  tmp = (64u) c;
  r += q[tmp];
  return r;
}

export fn small_range(reg u64 x) -> reg u8 {
  inline int i;
  stack u8[9] s;
  for i = 0 to 9 { s[i] = i; }
  s[0] = x;
  x &= 0xFF;
  reg u64 j;
  ?{}, j = #POPCNT(x);
  reg u8 r = s[j];
  return r;
}
