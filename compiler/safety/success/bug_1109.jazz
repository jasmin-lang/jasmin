param int SHAKE128_RATE = 168;

export
fn test() -> reg u64 {
  reg bool cond;
  reg u64 pos = 0;
  reg u64 res = 0;
  while { cond = (pos < SHAKE128_RATE - 2); } (cond) {
    pos += 3;
    res += 1;
  }
  return res;
}

export
fn non_regression() {
  stack u8[SHAKE128_RATE] array;
  reg u64 i = 0;
  reg u64 j = i;
  while (i <s SHAKE128_RATE >> 1) {
    array[j] = i;
    array[j + 1] = j;
    i += 1;
    j += 2;
  }
}
