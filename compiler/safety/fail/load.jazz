export
fn load_last(reg u64 ptr, reg u64 len) -> reg u64[2]
{
  reg   u64    j;
  reg   u64[2] x;
  stack u64[2] s;
  reg   u8     c;

  s[0] = 0;
  s[1] = 0;

  j = 0;
  while(j < len)
  { c = (u8)[ptr + j];
    s[u8 (int)j] = c;
    j += 1;
  }

  s[u8 (int)j] = 0x1;

  x[0] = s[0];
  x[1] = s[1];

  return x;
}
