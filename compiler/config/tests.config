[default]
bin = ./scripts/compile-and-assemble

[test-x86-64_nolea]
args   = -nolea
kodirs = tests/fail/nolea/x86-64

[test-safety]
bin    = ./scripts/check-safety
okdirs = !safety/success
kodirs = !safety/fail

[test-CCT]
bin    = ./scripts/check-cct
okdirs = !CCT/success
kodirs = !CCT/fail

[test-x86-64_all]
okdirs = !examples tests/success/**/x86-64
kodirs = tests/fail/x86-64 tests/fail/annotation/**/x86-64

[test-x86-64_print]
bin = ./scripts/parse-print-parse
okdirs = tests/success/x86-64

[test-x86-64_extraction]
bin = ./scripts/extract-and-check
okdirs = !examples tests/success/**/x86-64
exclude = tests/success/noextract

[test-arm-m4_all]
bin = ./scripts/check-arm
okdirs = tests/success/**/arm-m4
