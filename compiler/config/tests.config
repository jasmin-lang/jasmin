[default]
bin = ./scripts/compile-and-assemble

[test-nolea]
args   = -nolea
kodirs = tests/fail/nolea

[test-safety]
bin    = ./scripts/check-safety
okdirs = !safety/success
kodirs = !safety/fail

[test-all]
okdirs = examples examples/gimli tests/success tests/success/pointers tests/success/subroutines
kodirs = tests/fail tests/fail/annotation
