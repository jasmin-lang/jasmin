[default]
bin = ./scripts/compile-and-assemble

[test-nolea]
args   = -nolea
kodirs = tests/fail/nolea

[test-safety]
bin    = ./scripts/check-safety
okdirs = !safety/success
kodirs = !safety/fail

[test-all]
okdirs = examples examples/gimli tests/success
kodirs = tests/fail

[test-print]
bin = ./scripts/parse-print-parse
okdirs = tests/success
